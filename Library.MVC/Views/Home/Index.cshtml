@{
    ViewBag.Title = "Home Page";
}



    <div id="homepageCarouselControls" role="listbox" class="row carousel slide" data-ride="carousel" data-interval="5000">
        <div class="carousel-inner">
            <div class="item active">
                <img class="d-block w-100" src="~/Content/Images/HomePage/library1.jpg" alt="First slide">
            </div>
            <div class="item">
                <img class="d-block w-100" src="~/Content/Images/HomePage/library2.jpg" alt="Second slide">
            </div>
        </div>
        <a class="carousel-control-prev" href="#homepageCarouselControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#homepageCarouselControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

